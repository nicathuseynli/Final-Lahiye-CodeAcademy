@model CreateFaqPageVM
<div class="container">
    <h1 class="text-center">Create FAQ Page</h1>
      <div class="row">
             <form asp-action="Create" method="post" class="forms-sample" enctype="multipart/form-data">
                 <div class="form-group">
                   <input asp-for="Title" class="form-control" placeholder="Title">
                 </div>
                 <div class="form-group">
                   <input asp-for="Question" class="form-control"placeholder="Question">
                 </div>      
                 <div class="form-group">
                   <input asp-for="Answer" class="form-control"placeholder="Answer">
                 </div>           
                 <button type="submit" class="btn btn-outline-success me-2">Create</button>
                 <button asp-action="Index" class="btn btn-outline-danger">Cancel</button>
             </form>
    </div>
</div>