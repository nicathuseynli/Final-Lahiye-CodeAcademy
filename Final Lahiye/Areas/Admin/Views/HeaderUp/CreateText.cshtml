@model CreateHeaderUpTextVM
<div class="container">
    <h1 class="text-center">Create Social Media</h1>
      <div class="row">
             <form asp-action="CreateText" method="post" class="forms-sample" enctype="multipart/form-data">
                 <div class="form-group">
                   <input asp-for="Text" class="form-control" placeholder="Text">
                 </div>
                 <button type="submit" class="btn btn-outline-success me-2">Create</button>
                 <button asp-action="TextIndex" class="btn btn-outline-danger">Cancel</button>
             </form>
    </div>
</div>
